choice=int(input("Enter Choice 1 to Add & 2 to Subtract & 3 to Multiply & 4-Divide\n"))
a=int(input("Enter 1st Number\n"))
b=int(input("Enter 2nd Number\n"))
if(choice==1):
    print("The value is",a+b)
if(choice==2):
    print("The value is",a-b)
if(choice==3):
    print("The value is",a*b)
if(choice==4):
    print("The value is",a/b)
